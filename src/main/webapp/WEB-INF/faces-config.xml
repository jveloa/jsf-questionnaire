    <!-- DynTabTracker, manager, etc... -->
    <managed-bean>
        <managed-bean-name>dynTabTracker</managed-bean-name>
        <managed-bean-class>cu.edu.mes.sigenu.training.web.config.spa.DynTabTracker</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
        <managed-property>
            <property-name>maxNumberOfTabs</property-name>
            <property-class>java.lang.Integer</property-class>
            <value>10</value>
        </managed-property>
        <managed-property>
            <property-name>initialTabs</property-name>
            <property-class>java.util.List</property-class>
            <list-entries>
                <value-class>cu.edu.mes.sigenu.training.web.config.spa.DynTab</value-class>
                <value>#{HomeDynTab}</value>
            </list-entries>
        </managed-property>
    </managed-bean>

